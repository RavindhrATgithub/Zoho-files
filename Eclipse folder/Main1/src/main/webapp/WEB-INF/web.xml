<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>serve1</servlet-name>
  	<servlet-class>pack.files.serveFile</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>serve2</servlet-name>
  	<servlet-class>pack.files.UrlInfo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>serve3</servlet-name>
  	<servlet-class>pack.files.IPFinder</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>serve4</servlet-name>
  	<servlet-class>pack.files.ImageCopy</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>serve5</servlet-name>
  	<servlet-class>pack.files.ImageCrop</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>serve1</servlet-name>
  	<url-pattern>/latency</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>serve2</servlet-name>
  	<url-pattern>/UrlInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>serve3</servlet-name>
  	<url-pattern>/IPFinder</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>serve4</servlet-name>
  	<url-pattern>/ImageCopy</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>serve5</servlet-name>
  	<url-pattern>/ImageCrop</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>NewFile.html</welcome-file>
  </welcome-file-list>
</web-app>
